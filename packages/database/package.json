    "googleapis": "^144.0.0",
    "google-auth-library": "^9.4.1"
  },
  "devDependencies": {
    "@types/postgres": "^3.0.4",
    "typescript": "^5.4.3",
    "tsx": "^4.7.1",
    "eslint": "^8.52.0"
  }
}