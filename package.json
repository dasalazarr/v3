{
  "name": "running-coach-workspace",
  "version": "1.0.0",
  "description": "AI-powered running coach with memory and adaptive training plans",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm --filter api-gateway dev",
    "start": "pnpm --filter api-gateway start",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "migrate": "pnpm --filter database migrate",
    "type-check": "pnpm -r type-check"
  },
  "keywords": [
    "running",
    "ai",
    "coaching",
    "whatsapp",
    "fitness"
  ],
  "devDependencies": {
    "@types/node": "^20.19.2",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.52.0",
    "nodemon": "^3.1.0",
    "reflect-metadata": "^0.2.1",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "vitest": "^1.0.0"
  },
  "author": "Running Coach Team",
  "license": "ISC",
  "dependencies": {
    "@types/dotenv": "^8.2.3",
    "api-gateway": "^1.0.0",
    "drizzle-orm": "^0.31.4",
    "franc-min": "^6.2.0",
    "iso-639-1": "^3.1.5"
  }
}
