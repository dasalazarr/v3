{
  "name": "running-coach-workspace",
  "version": "1.0.0",
  "description": "AI-powered running coach with memory and adaptive training plans",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm --filter api-gateway dev",
    "start": "pnpm --filter api-gateway start",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "migrate": "pnpm --filter database migrate",
    "type-check": "pnpm -r type-check"
  },
  "keywords": [
    "running",
    "ai",
    "coaching",
    "whatsapp",
    "fitness"
  ],
  "devDependencies": {
    "@types/node": "^20.19.2",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.4.0",
    "depcheck": "^1.4.7",
    "eslint": "^8.52.0",
    "eslint-plugin-builderbot": "^1.2.9",
    "nodemon": "^3.1.0",
    "reflect-metadata": "^0.2.1",
    "ts-prune": "^0.10.3",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "vitest": "^1.0.0"
  },
  "author": "Running Coach Team",
  "license": "ISC"
}
